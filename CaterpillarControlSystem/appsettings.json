{
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console", "Serilog.Sinks.File" ],
    "MinimumLevel": "Debug",
    "WriteTo": [
      {
        "Name": "Console"
      },
      {
        "Name": "File",
        "Args": {
          "path": "logs.txt"
        }
      }
    ],
    "Properties": {
      "ApplicationName": "Caterpillar Control System"
    }
  }
}